<!DOCTYPE html>
<html>
<head>
  <title>Rent Street</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body class = '<%= controller.controller_name %>'>
  <div id="banner">
    <%= image_tag("logo.png") %>
    <h1 class="banner_title"><%= @page_title || "Rent Street" %></h1>
  </div>
  <div id="columns">
    <div id="side">
      <div id="cart">
        <% if @cart %>
        <%= render @cart %>
        <% end %>
      </div>
      <ul>
        <li><a href="http://localhost:3000/"> Home </a></li>
        <li><a> Questions </a></li>
        <li><a> News </a></li>
        <li><a> Contact </a></li>
      </ul>
    </div>
    <div id="main">



<%= yield %>
</div>
</div>

</body>
</html>
